{
  "manifest_version": 3,
  "name": "Cookie Changer",
  "version": "1.0",
  "description": "Change cookies on websites!!",
  "permisstions": ["cookies", "activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Change Cookies"
  }
}
